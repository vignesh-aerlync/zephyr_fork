/dts-v1/;

#include <nxp/nxp_lpc845.dtsi>

/ {
	model = "NXP LPC845-BRK Board";
	compatible = "nxp,lpc845-brk";

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
